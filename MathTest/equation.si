//求一元二次方程：ax2+bx+c=0 的根。
//输入三个实数a,b,c的值，且a不等于0。

main()
{
    //方程的三个系数
    a=2;
    b=1;
    c=-6;
	if(a!=0)
	{
		d=_fun("math_sqrt",b*b-4*a*c);
		x1=(0-b+d)/(2*a);
		x2=(0-b-d)/(2*a);
		if(x1<x2) 
			printf("%0.2f %0.2f\n",x2,x1); 
		else
			printf("%0.2f %0.2f\n",x1,x2);
	}	
        
}